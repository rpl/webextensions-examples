{
  "background": {
    "scripts": ["background.js"]
  },
  "description": "A page action to switch to the source tab from a pageAction.",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/back-to-the-sourcetab",
  "page_action": {
    "default_title": "Back to the source tab"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "manifest_version": 2,
  "name": "Back to the Source Tab",
  "version": "1.0.5",

  "applications": {
    "gecko": {
      "id": "backtosourcetab@test.mozilla.org",
      "strict_min_version": "54.0a1"
    }
  }
}
